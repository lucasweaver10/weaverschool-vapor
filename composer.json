{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4||^8.0",
        "doctrine/dbal": "^3.0",        
        "google/apiclient": "^2.16",
        "google/cloud-text-to-speech": "^1.7",
        "google/cloud-translate": "^1.17",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/cashier": "^15.2.2",
        "laravel/fortify": "^1.19",
        "laravel/framework": "^10.47",
        "laravel/horizon": "^5.24",        
        "laravel/sanctum": "^3.3",
        "laravel/telescope": "^4.17.3",
        "laravel/tinker": "^2.8.2",
        "laravel/vapor-core": "^2.37",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^3.4.2",
        "mixpanel/mixpanel-php": "^2.11",
        "mollie/laravel-mollie": "^2.0",
        "moontoast/math": "1.1.2 as 1.999.999",
        "nadar/stemming": "^1.1",
        "openai-php/client": "^0.8.4",
        "php-ffmpeg/php-ffmpeg": "^1.1",
        "predis/predis": "^2.2",
        "segmentio/analytics-php": "^3.6",
        "sentry/sentry-laravel": "^4.7",
        "spatie/image": "^3.6",
        "spatie/image-optimizer": "^1.7",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-ignition": "^2.3.3",
        "spatie/laravel-image-optimizer": "^1.8",
        "spatie/laravel-medialibrary": "^11.5",
        "spatie/laravel-sitemap": "^7.2.0",
        "spatie/laravel-translatable": "6.5.5",
        "spatie/pdf-to-text": "^1.52",
        "stripe/stripe-php": "^13.12.0",
        "thiagoalessio/tesseract_ocr": "^2.13",
        "vimeo/vimeo-api": "^3.0",
        "wamania/php-stemmer": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "laravel/breeze": "^1.3",
        "laravel/dusk": "^7.12.0",
        "laravel/sail": "^1.25",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.34.7",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [{
        "type": "composer",
        "url": "https://nova.laravel.com"
    }]
}
